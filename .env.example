# Example .env file for GitHub Actions Secrets Uploader
# Copy this file to .env and fill in your actual values

# API Keys
API_KEY=your-secret-api-key-here
STRIPE_API_KEY=sk_test_your_stripe_key_here
OPENAI_API_KEY=sk-your-openai-key-here

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/myapp
REDIS_URL=redis://localhost:6379/0

# Authentication
JWT_SECRET=your-super-secret-jwt-signing-key
SESSION_SECRET=your-session-secret-key

# External Services
WEBHOOK_SECRET=your-webhook-secret-from-service
SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
EMAIL_API_KEY=your-email-service-api-key

# Feature Flags
FEATURE_BETA_ENABLED=true
DEBUG_MODE=false
MAINTENANCE_MODE=false

# Third-party Integrations
# Note: GITHUB_TOKEN cannot be used as secret name (GitHub restriction)
# Use GH_TOKEN, PERSONAL_ACCESS_TOKEN, or similar instead
GH_TOKEN=ghp_your_github_personal_access_token
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/SLACK/WEBHOOK
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/your/webhook

# Environment-specific
ENVIRONMENT=production
LOG_LEVEL=info
PORT=3000

# AWS Configuration (if needed)
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=us-east-1
S3_BUCKET_NAME=your-s3-bucket-name

# Google Cloud (if needed)
GOOGLE_APPLICATION_CREDENTIALS_JSON={"type":"service_account","project_id":"your-project"}

# Security
ENCRYPTION_KEY=your-32-character-encryption-key
CORS_ORIGIN=https://yourdomain.com

# Monitoring
NEW_RELIC_LICENSE_KEY=your-new-relic-key
DATADOG_API_KEY=your-datadog-api-key